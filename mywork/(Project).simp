                <operation id="oac46fae01f804277864f3adfd323dcb0412" cid="OP063" name="checkForCheck" creation-date="12/11/2023 03:27:33" type="" type-id="" visibility="public">
                  <tagged-values>
                    <tagged-value id="tvc628d20580a040b0a82e74b6a94e55de" name="ReverseEngineered" type="text" multiple="false" hidden="true" value="checkForCheck" />
                    <tagged-value id="tvec0d0a0c51634846bfed62b133e631d8" name="SourceCodePath" type="text" multiple="false" hidden="true" value="E:\kursovaya3sem\myProject\chessNK\chessbox.h" />
                    <tagged-value id="tv03d7ddbcd72d487bb75e0e9515369af4" name="SourceCodeLanguage" type="text" multiple="false" hidden="true" value="Cpp" />
                    <tagged-value id="tvf30d0caba28d4397ab01f1513a8820c3" name="SourceCodePosition" type="text" multiple="false" hidden="true" value="653" />
                    <tagged-value id="tv1b2ebcc57b67439eb7231b69918acd72" name="SourceCodeLength" type="text" multiple="false" hidden="true" value="21" />
                  </tagged-values>
                  <source-codes>
                    <source-code language="Cpp">
    int c = 0;
    QList &lt;ChessPiece *&gt; pList = game-&gt;alivePiece;
    for(size_t i = 0,n=pList.size(); i &lt; n; i++ ) {

        King * p = dynamic_cast&lt;King *&gt; (pList[i]);
        if(p){
            continue;
        }
        pList[i]-&gt;moves();
        pList[i]-&gt;decolor();
        QList &lt;ChessBox *&gt; bList = pList[i]-&gt;moveLocation();
        for(size_t j = 0,n = bList.size(); j &lt; n; j ++) {
            King * p = dynamic_cast&lt;King *&gt; (bList[j]-&gt;currentPiece);
            if(p) {
                if(p-&gt;getSide() == pList[i]-&gt;getSide())
                    continue;
                bList[j]-&gt;setColor(Qt::darkMagenta);

                                                                                
                                            
                      
                                            
                                   
                                                                      
                if(!game-&gt;check-&gt;isVisible())
                    game-&gt;check-&gt;setVisible(true);
                else{
                    bList[j]-&gt;resetOriginalColor();
                    pList[i]-&gt;getCurrentBox()-&gt;resetOriginalColor();
                                       
                }
                c++;

            }
        }
    }
    if(!c){
        game-&gt;check-&gt;setVisible(false);
        for(size_t i = 0,n=pList.size(); i &lt; n; i++ )
            pList[i]-&gt;getCurrentBox()-&gt;resetOriginalColor();
    }
</source-code>
                  </source-codes>
                </operation>
