                <operation id="ofa664ffacb2241f582364650541f1125413" cid="OP083" name="moves" creation-date="12/11/2023 03:27:33" type="" type-id="" visibility="public">
                  <tagged-values>
                    <tagged-value id="tvc8aab36785c74e63b5dbbddc6d7129fe" name="ReverseEngineered" type="text" multiple="false" hidden="true" value="moves" />
                    <tagged-value id="tv5c9d34aa3b23446fb39cc35f809d4683" name="SourceCodePath" type="text" multiple="false" hidden="true" value="E:\kursovaya3sem\myProject\chessNK\queen.h" />
                    <tagged-value id="tv4d6fc0c152634f0982381be0f8539aad" name="SourceCodeLanguage" type="text" multiple="false" hidden="true" value="Cpp" />
                    <tagged-value id="tv499261f48fb74e7a8f71ebb8508ec781" name="SourceCodePosition" type="text" multiple="false" hidden="true" value="184" />
                    <tagged-value id="tv43ec8b9af03541069896d5b4149845d1" name="SourceCodeLength" type="text" multiple="false" hidden="true" value="13" />
                  </tagged-values>
                  <source-codes>
                    <source-code language="Cpp">
    location.clear();
    int row = this-&gt;getCurrentBox()-&gt;rowLoc;
    int col = this-&gt;getCurrentBox()-&gt;colLoc;
    QString team = this-&gt;getSide();
            

     for(int i = row-1,j = col; i &gt;= 0 ; i--) {
       if(game-&gt;collection[i][j]-&gt;getChessPieceColor() == team ) {
           break;
       }
       else
       {
           location.append(game-&gt;collection[i][j]);
           if(boxSetting(location.last()))
               break;
        }
    }

               

      for(int i = row+1,j = col; i &lt;= 7 ; i++) {
        if(game-&gt;collection[i][j]-&gt;getChessPieceColor() == team ) {
            break;
        }
        else
        {
            location.append(game-&gt;collection[i][j]);
            if(boxSetting(location.last())){
                break;
            }
        }
     }

                

       for(int i = row,j = col-1; j &gt;= 0 ; j--) {
         if(game-&gt;collection[i][j]-&gt;getChessPieceColor() == team ) {
             break;
         }
         else
         {
             location.append(game-&gt;collection[i][j]);
             if(boxSetting(location.last()))
                break;
         }
        }
                  

        for(int i = row,j = col+1; j &lt;= 7 ; j++)
        {
              if(game-&gt;collection[i][j]-&gt;getChessPieceColor() == team ) {
                  break;
              }
              else
              {
                  location.append(game-&gt;collection[i][j]);
                  if(boxSetting(location.last()))
                      break;
               }

       }

                        

         for(int i = row-1,j = col-1; i &gt;= 0 &amp;&amp; j &gt;=0; i--,j--) {
           if(game-&gt;collection[i][j]-&gt;getChessPieceColor() == team ) {
               break;

           }
           else
           {
               location.append(game-&gt;collection[i][j]);
               if(boxSetting(location.last()) ){
                   break;
               }
           }
        }

                          

          for(int i = row-1,j = col+1; i &gt;= 0 &amp;&amp; j &lt;= 7; i--,j++) {
            if(game-&gt;collection[i][j]-&gt;getChessPieceColor() == team ) {
                break;

            }
            else
            {
                location.append(game-&gt;collection[i][j]);
                if(boxSetting(location.last())){
                    break;
                }
            }
         }

                              

           for(int i = row+1,j = col+1; i &lt;= 7 &amp;&amp; j &lt;= 7; i++,j++) {
             if(game-&gt;collection[i][j]-&gt;getChessPieceColor() == team ) {
                 break;

             }
             else
             {
                 location.append(game-&gt;collection[i][j]);
                 if(boxSetting(location.last())){
                     break;
                 }
             }
          }

                              

            for(int i = row+1,j = col-1; i &lt;= 7 &amp;&amp; j &gt;= 0; i++,j--) {
              if(game-&gt;collection[i][j]-&gt;getChessPieceColor() == team ) {
                  break;

              }
              else
              {
                  location.append(game-&gt;collection[i][j]);
                  if(boxSetting(location.last())){
                      break;
                  }

              }
           }


</source-code>
                  </source-codes>
                </operation>
              </operations>
